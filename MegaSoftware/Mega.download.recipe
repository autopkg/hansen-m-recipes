<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest version of Mega.</string>
    <key>Identifier</key>
    <string>com.github.hansen-m.download.mega</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>Mega</string>
        <key>SEARCH_URL</key>
        <string>http://www.megasoftware.net/releases/?C=M;O=D</string>
        <key>SEARCH_PATTERN</key>
        <string>MEGA(?P&lt;version&gt;[0-9\.]+)_mac32_setup\.dmg</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.9</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%SEARCH_URL%</string>
                <key>re_pattern</key>
                <string>%SEARCH_PATTERN%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>http://www.megasoftware.net/releases/MEGA%version%_mac32_setup.dmg</string>
                <key>filename</key>
                <string>MEGA%version%_mac32_setup.dmg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
